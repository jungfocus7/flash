package hbworks.uilogics.subClasses
{
	public final class AlignInfo
	{
		public function AlignInfo(
							width:Number = 400,
							marginX:Number = 10, marginY:Number = 10)
		{
			this.width = width;
			this.marginX = marginX;
			this.marginY = marginY;
		}

		// - width
		public var width:Number;
		
		// - margin x
		public var marginX:Number;
		// - margin y
		public var marginY:Number;
	}
}
